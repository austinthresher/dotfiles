 '(window-divider-first-pixel ((t (:foreground "#1A1D20"))))
 '(window-divider-last-pixel ((t (:foreground "#1A1D20"))))
 '(tab-line ((t (:underline (:color "#4D4D5F" :style line :position 0) :height 0.9 :foreground "#8D8DAF" :background "#0A0D10" :inherit variable-pitch))))
 '(tab-line-tab ((t (:inherit tab-line-tab-current))))
 '(tab-line-tab-current ((t (:height 1.0 :underline (:color "#1A1D20" :style line :position 0) :box (:line-width (1 . 1) :color "#2D2D2F" :style flat-button) :foreground "#DDDDF0" :background "#1A1D20" :inherit tab-line-tab))))
 '(tab-line-tab-inactive ((t (:background "#2D2D38" :foreground "#8D8D98" :inherit tab-line-tab :underline (:color "#4D4D5F" :style line :position 0)))))
 '(tab-line-highlight ((t (:height 1.0 :inherit highlight))))
 '(orderless-match-face-0 ((t (:weight bold :foreground "#FFFFFF" :underline "#72A4FF"))))
 '(orderless-match-face-1 ((t (:weight bold :foreground "#FFFFFF" :underline "#ed92f8"))))
 '(orderless-match-face-2 ((t (:weight bold :foreground "#FFFFFF" :underline "#90d800"))))
 '(orderless-match-face-3 ((t (:weight bold :foreground "#FFFFFF" :underline "#f0ce43"))))
 '(hl-line ((t (:background "#333340" :extend t :underline (:color "black" :position 0)))))
 '(default ((t (:inherit nil :extend nil :stipple nil :background "#1A1D20" :foreground "#DDDDE8" :inverse-video nil :box nil :strike-through nil :overline nil :underline nil :slant normal :weight regular :height 130 :width normal :foundry "UKWN" :family "Iosevka NFP"))))
 '(eshell-prompt ((t (:foreground "#88BBDD" :weight medium))))
 )

(provide-theme 'mostly-mono)
