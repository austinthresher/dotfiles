{
    "diagnostic.enableSign": true,
    "diagnostic.errorSign": "⮞",
    "diagnostic.warningSign": "⮞",
    "diagnostic.infoSign": "·",
    "diagnostic.hintSign": "·",
    "diagnostic.checkCurrentLine": true,
    "diagnostic.virtualText": false,
    "diagnostic.virtualTextLimitInOneLine": 1,
    "diagnostic.virtualTextAlign": "right",
    "diagnostic.virtualTextCurrentLineOnly": false,
    "inlayHint.display": false,
    "suggest.selection": "first",
    "suggest.noselect": true,
    "coc.source.file.enable": false,
    "coc.source.around.enable": false,
    "coc.source.buffer.enable": false,
    //"[c][cpp]": {
    //    "semanticTokens.enable": true
    //},
    "inlayHint.enable": true,
    "tree.key.activeFilter": "<S-f>",
    "tree.key.actions": "<cr>",
    "workspace.openResourceCommand": "",
    "clangd.arguments": ["--enable-config"],
    "lightbulb.text": {
        //"default": "",
        //"quickfix": ""
        "default": "",
        "quickfix": ""
    }
}
